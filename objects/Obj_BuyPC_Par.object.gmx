draw_set_halign(fa_center);
draw_set_valign(fa_top );

mx = mouse_x;
my = mouse_y+4;
if(!buyed){
    image_blend = c_black;
    if(place_meeting(x,y,Obj2_Cursor) and clicked == false){
        depth = -10;
        image_alpha = 1;
        draw_text_colour(mx, my, "-"+string(name)+"-",c_red   ,c_red   ,c_blue   ,c_red   ,1);
        draw_text_colour(mx, my,"#" +string(desc)    ,c_white ,c_white ,c_white  ,c_white ,1);
        if(pric &lt; 1000){
            draw_text_colour(mx, my,"##"+"$"+string(pric),c_yellow,c_red   ,c_yellow ,c_white ,1);
        }else if (pric &gt;= 1000 and pric &lt; 1000000){
            draw_text_colour(mx, my,"##"+"$"+string(pric/1000)+"K",c_yellow,c_red   ,c_yellow ,c_white ,1);
        }else if (pric &gt;= 1000000 and pric &lt; 1000000000){
            draw_text_colour(mx, my,"##"+"$"+string(pric/1000000)+"M",c_yellow,c_red   ,c_yellow ,c_white ,1);
        }
        
        if(mouse_check_button_pressed(mb_left)) and global.money &gt;= pric{
            global.money -= pric;
            with(instance_create(x,y,Obj2_TimePC)){
                other._time_id = id;
                time = other.time;
                other.buyed_2 = true;
                other.clicked = true;
            }
        }
    }else{
        depth = 0;
        image_alpha += (0.5 - image_alpha)/20;
    }
    if(buyed_2){
        image_alpha = 1;
        if(_time_id.time &lt;= 0){
            buyed = true;
            with(_time_id){
                instance_destroy();
            }
        }
    }
}else{
    if(place_meeting(x,y,Obj2_Cursor) and global.pc_temp &lt;= 0){
        depth = -10;
        if(selected_pc_2 == false){
            draw_text_colour(mx,my,"SELECT"  ,c_red,c_red,c_red,c_red,1);
            if(mouse_check_button_pressed(mb_left)){
                with(all){selected_pc_2 = false;}
                selected_pc_2 = true;
                global.pc_os = SO;
            }
        }else{
            depth = 10;
            draw_text_colour(mx,my,"SELECTED",c_blue,c_blue,c_blue,c_blue,1);
        }
    }
    if(global.pc_temp &gt; 0){
        if(place_meeting(x,y,Obj2_Cursor)){
            draw_text_colour(mx,my,"HOT PC",c_red,c_red,c_red,c_purple,1);
        }
    }
    if(selected_pc_2){
        image_blend = c_green;
    }else{
        image_blend = c_red;
    }
}

draw_set_halign(fa_left);
draw_set_valign(fa_top );
